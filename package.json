{
  "name": "astart-cli",
  "version": "1.9.0",
  "author": "YanPanMichael",
  "repository": {
    "type": "git",
    "url": "https://github.com/YanPanMichael/astart-cli.git"
  },
  "description": "A modern web application scaffold with multiple solution templates",
  "main": "bin/cli.js",
  "files": [
    "bin",
    "packages",
    "constants",
    "CHANGELOG.md"
  ],
  "bugs": {
    "url": "https://github.com/YanPanMichael/astart-cli/issues",
    "email": "1214863281@qq.com"
  },
  "bin": {
    "astart": "bin/cli.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "node",
    "cli",
    "bin",
    "packages"
  ],
  "scripts": {
    "format": "prettier --write .",
    "astart": "node ./bin/cli.js",
    "release": "qkrelease --skipBuild",
    "fix": "eslint . --fix",
    "posttest": "eslint .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "@types/node": "^14.14.41",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "enquirer": "^2.3.6",
    "execa": "^5.1.1",
    "ignore": "^5.2.4",
    "minimist": "^1.2.7",
    "prettier": "^2.5.1",
    "progress": "^2.0.3",
    "single-line-log": "^1.1.2"
  },
  "directories": {
    "doc": "docs"
  },
  "license": "MIT",
  "devDependencies": {
    "docsify-cli": "^4.4.4",
    "esbuild-wasm": "^0.15.12",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "qk-release": "latest",
    "vitepress": "1.0.0-alpha.26",
    "vue": "^3.2.41"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "npm:esbuild-wasm@^0.15.12"
    }
  }
}
